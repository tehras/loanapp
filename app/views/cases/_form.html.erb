<%= simple_form_for(@case) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :reason_for_end %>
    <%= f.input :pay_amount %>
    <%= f.input :invoice %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
